{
  "name": "@superlanding/beyond",
  "version": "0.0.189",
  "description": "The best frontend framework for super-landing.com",
  "browserslist": [
    "defaults",
    "ie >= 9"
  ],
  "main": "dist/beyond.js",
  "scripts": {
    "version": "echo $npm_package_version",
    "dev": "webpack-dev-server --host 0.0.0.0",
    "build": "./build.sh",
    "deploy": "./deploy.sh",
    "release": "./release.sh",
    "test": "nyc ava src/js/tests/**/*.test.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/superlanding/beyond.git"
  },
  "author": "kmsheng",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/superlanding/beyond/issues"
  },
  "homepage": "https://github.com/superlanding/beyond#readme",
  "ava": {
    "require": [
      "module-alias/register",
      "./test/setup.js"
    ],
    "babel": {
      "compileAsTests": [
        "./src/js/**/*.js"
      ]
    }
  },
  "_moduleAliases": {
    "~": "."
  },
  "nyc": {
    "extension": [
      ".js"
    ]
  },
  "dependencies": {
    "@superlanding/datetotimestamp": "0.0.22",
    "@superlanding/getdompos": "0.0.22",
    "@superlanding/getscrollleft": "0.0.22",
    "@superlanding/getscrolltop": "0.0.22",
    "@superlanding/timestamptodate": "0.0.22",
    "@superlanding/topixel": "0.0.22",
    "core-js": "3.6.4",
    "date-fns": "2.9.0",
    "date-fns-tz": "1.0.9",
    "date-time-format-timezone": "1.0.21",
    "element-dataset": "2.2.6",
    "intl": "1.2.5",
    "lodash.debounce": "4.0.8",
    "lodash.isfunction": "3.0.9",
    "lodash.noop": "3.0.1",
    "lodash.range": "3.2.0",
    "lodash.sample": "4.2.1",
    "lodash.sortby": "4.7.0",
    "lodash.throttle": "4.1.1",
    "lodash.uniqby": "4.7.0",
    "mem": "6.1.1",
    "node.normalize.scss": "3.0.3",
    "regenerator-runtime": "0.13.3"
  },
  "devDependencies": {
    "@ava/babel": "1.0.1",
    "@babel/core": "7.8.3",
    "@babel/plugin-proposal-decorators": "7.8.3",
    "@babel/preset-env": "7.8.3",
    "autoprefixer": "9.7.3",
    "ava": "3.10.1",
    "babel-eslint": "10.0.3",
    "babel-loader": "8.0.6",
    "css-loader": "3.4.2",
    "eslint": "6.8.0",
    "eslint-config-google": "0.14.0",
    "eslint-loader": "3.0.3",
    "file-loader": "5.0.2",
    "jsdom": "16.3.0",
    "jsdom-global": "3.0.2",
    "mini-css-extract-plugin": "0.9.0",
    "module-alias": "2.2.2",
    "node-sass": "4.14.1",
    "nyc": "15.1.0",
    "postcss-loader": "3.0.0",
    "sass-loader": "9.0.2",
    "svg-url-loader": "3.0.3",
    "webpack": "4.42.0",
    "webpack-cli": "3.3.10",
    "webpack-dev-server": "3.10.1"
  },
  "resolutions": {
    "loader-fs-cache/mkdirp": "0.5.3"
  },
  "publishConfig": {
    "access": "public"
  }
}
